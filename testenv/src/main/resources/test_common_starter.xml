<beans xmlns="http://www.springframework.org/schema/beans"
       xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
       xsi:schemaLocation="http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans-4.3.xsd">

    <bean id="startEventListener" class="org.webvane.common.spring.StartEventListener">
        <constructor-arg>
            <list>
                <bean id="componentInitAppStarter" class="org.webvane.common.spring.starters.ComponentInitAppStarter"/>
                <bean id="initializableAppStarter" class="org.webvane.common.spring.starters.InitializableAppStarter"/>
            </list>
        </constructor-arg>
    </bean>
</beans>
